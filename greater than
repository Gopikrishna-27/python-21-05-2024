#include <stdio.h>

int main() {
    int n;
    
    // Prompt user for input
    printf("Enter an integer (number should have more than 2 digits): ");
    scanf("%d", &n);

    // Check if the number has more than 2 digits
    if (n > 99 || n < -99) {
        // Extract the last two digits
        int last_two_digits = n % 100;
        
        // Handle negative numbers to print positive last two digits
        if (last_two_digits < 0) {
            last_two_digits = -last_two_digits;
        }

        // Print the result
        printf("The last two digits of %d are: %02d\n", n, last_two_digits);
    } else {
        printf("The number does not have more than 2 digits.\n");
    }

    return 0;
}
